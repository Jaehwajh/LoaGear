                    </label>
                    <select class="w-full rounded-md border border-[#e0e0e0] bg-white py-3 px-6 text-base font-medium text-[#6B7280] outline-none focus:border-[#6A64F1] focus:shadow-md" name="negativeEngraving" id="negativeEngraving">
                      <% nEngravings.forEach(element => { %>
                          <option value="<%- element %>"><%- element %></option>
                      <% }); %>
                  </select>
                  </div>
                  <div class="mb-5">
                    <label for="subject" class="mb-3 block text-base font-medium text-[#07074D]">
                      Character Name
                    </label>
                    <input
                      type="text"
                      name="character"
                      id="character"
                      placeholder=""
                      class="w-full rounded-md border border-[#e0e0e0] bg-white py-3 px-6 text-base font-medium text-[#6B7280] outline-none focus:border-[#6A64F1] focus:shadow-md"
                    />
                  </div>
                <div>
                  <button class="hover:shadow-form rounded-md bg-gray-800 py-3 px-8 text-base font-semibold text-white outline-none">
                    Submit
                  </button>
                </div>
              </form>
            </div>
          </div>
        <!-- Add bracelet -->
        <div>
            <form action="">
    
            </form>
        </div>
    </div>
</div>
<!-- Display Accessories, stones, bracelets -->
<div>
