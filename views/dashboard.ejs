                    <% nEngravings.forEach(element => { %>
                        <option value="<%- element %>"><%- element %></option>
                    <% }); %>
                </select>
                </div>
                <div class="mb-5 px-4">
                  <label for="negativeNode" class="mb-3 block text-base font-medium text-[#07074D]">
                    Value
                  </label>
                  <input
                    type="text"
                    name="negativeNode"
                    id="negativeNode"
                    placeholder=""
                    class="w-full rounded-md border border-[#e0e0e0] bg-white py-3 px-6 text-base font-medium text-[#6B7280] outline-none focus:border-[#6A64F1] focus:shadow-md"
                  />
                </div>
              </div>

              <div class="mb-5">
                <label for="character" class="mb-3 block text-base font-medium text-[#07074D]">
                  Character Name
                </label>
                <input
                  type="text"
                  name="character"
                  id="character"
                  placeholder=""
                  class="w-full rounded-md border border-[#e0e0e0] bg-white py-3 px-6 text-base font-medium text-[#6B7280] outline-none focus:border-[#6A64F1] focus:shadow-md"
                />
              </div>
            <div>
              <button class="hover:shadow-form rounded-md bg-gray-800 py-3 px-8 text-base font-semibold text-white outline-none">
                Submit
              </button>
            </div>
          </form>
        </div>
      </div>
        <!-- Add stone -->
        <div class="flex items-center justify-center p-12 hidden">
            <div class="mx-auto w-full max-w-[550px]">
              <form action="/dashboard/createStone" method="POST">
                <div class="mb-5">
                  <label for="engravingOne" class="mb-3 block text-base font-medium text-[#07074D]">
