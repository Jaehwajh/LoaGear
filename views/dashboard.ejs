                    <% bEngravings.forEach(element => { %>
                        <option value="<%- element %>"><%- element %></option>
                    <% }); %>
                </select>
                </div>
                <div class="mb-5">
                    <label for="engravingTwo" class="mb-3 block text-base font-medium text-[#07074D]">
